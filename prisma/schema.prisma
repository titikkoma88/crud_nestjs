generator client {
  provider = "prisma-client"
  output   = "../generated/prisma"
}

datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
}

model User {
  id Int @id @default(autoincrement())
  name String
  email String @unique
  username String
  createdAt DateTime @default(now()) @map(name: "created_at")
  updateAt DateTime @updatedAt @map(name: "update_at")

  @@map(name: "users")
}
